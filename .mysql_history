create table list_pets
(ID INT(11) NOT NULL AUTO_INCREMENT, Species VARCHAR(32) NOT NULL, Name VARCHAR(32) NOT NULL, Owner_ID INT(11) NOT NULL, PRIMARY KEY (ID), CONSTRAINT FK_list_pets FOREIGN KEY (Owner_ID) REFERENCES list_people (ID));
create table list_pets (ID INT(11) NOT NULL AUTO_INCREMENT, Species VARCHAR(32) NOT NULL, Name VARCHAR(32) NOT NULL, Owner_ID INT(11) NOT NULL, PRIMARY KEY (ID), CONSTRAINT FK_list_pets FOREIGN KEY (Owner_ID) REFERENCES list_people (ID));
create table pets_owners
(ID INT(11) NOT NULL AUTO_INCREMENT, Species VARCHAR(32) NOT NULL, Animal_Name VARCHAR(32) NOT NULL, Last_Name VARCHAR(32) NOT NULL, First_Name VARCHAR(32) NOT NULL, Owner_ID INT(11) NOT NULL, PRIMARY KEY (ID), CONSTRAINT FK_pets_owners FOREIGN KEY (Owner_ID) REFERENCES list_people (ID));
create table pets_owners (ID INT(11) NOT NULL AUTO_INCREMENT, Species VARCHAR(32) NOT NULL, Animal_Name VARCHAR(32) NOT NULL, Last_Name VARCHAR(32) NOT NULL, First_Name VARCHAR(32) NOT NULL, Owner_ID INT(11) NOT NULL, PRIMARY KEY (ID), CONSTRAINT FK_pets_owners FOREIGN KEY (Owner_ID) REFERENCES list_people (ID));
